	    $count : ArrayList(size > 1) from collect($sensor: Sensor(subSystem matches "Propulsion", type matches "Voltage", (value < 27 || value > 32)))
	    $sensorOut : Sensor(subSystem matches "Propulsion", type matches "Voltage", (value < 27 || value > 32))
	then
		Observation o = new Observation(Observation.PropulsionVoltageSubSystemFailure, $sensorOut.getSubSystem());
		insert(o);
end

